{
  "name": "nodejs-design-patterns",
  "version": "1.0.0",
  "description": "Node Tech Talk",
  "main": "app.js",
  "scripts": {
    "build": "babel 10-scalability-and-architectural-patterns -d build --copy-files",
    "babelRun": "babel-node 10-scalability-and-architectural-patterns/es2017app"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "ie": "8"
          }
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ulminator/nodejs-design-patterns.git"
  },
  "author": "Matt Ulmer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ulminator/nodejs-design-patterns/issues"
  },
  "homepage": "https://github.com/Ulminator/nodejs-design-patterns#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
